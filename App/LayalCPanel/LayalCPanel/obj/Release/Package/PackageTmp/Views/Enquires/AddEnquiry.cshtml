@{
    ViewBag.Title = Token.AddEnquiry;
    Layout = "~/Views/Shared/_Layout_Anonymous.cshtml";
}

<div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor" ng-controller="enquiresCtrl">

    <!-- end:: Subheader -->
    <div class="kt-content  kt-grid__item kt-grid__item--fluid">
        <div class="row">
            <div class="col-lg-12">
                <!--AddEnquiry-->
                <div class="kt-portlet">
                    <div class="kt-portlet__head">
                        <div class="kt-portlet__head-label">
                            <span class="kt-portlet__head-icon">
                                <i class="kt-font-brand flaticon-add"></i>
                            </span>
                            <h3 class="kt-portlet__head-title">@Token.AddNewEnquiry</h3>
                        </div>
                    </div>

                    <!--AddEnquiry Form-->
                    @Html.Partial("~/Views/Enquires/_AddAndUpdatePartial.cshtml")
                    
                </div>

                <!--end::Portlet-->
            </div>
        </div>
    </div>

</div>
@section scripts{
    <!--(ANageeb) link angualr files for thsis  view -->
    <script src="~/Scripts/angularJS/views/enquires/addEnquiry/service.js"></script>
    <script src="~/Scripts/angularJS/views/enquires/addEnquiry/contoller.js"></script>
}